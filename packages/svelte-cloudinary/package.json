{
	"name": "svelte-cloudinary",
	"version": "1.3.2",
	"author": "Matias Hernandez <hola@matiashernandez.dev>",
	"contributors": [
		"Matias Hernandez <hola@matiashernandez.dev>",
		"Willow (GHOST) <ghostdevbusiness@gmail.com> (https://ghostdev.xyz)",
		"Colby Fayock <hello@colbyfayock.com>"
	],
	"license": "MIT",
	"scripts": {
		"dev": "svelte-package --watch --input src",
		"build": "svelte-package --input src",
		"prepublishOnly": "cp ../../README.md .",
		"postpublish": "rm ./README.md",
		"postinstall": "node scripts/version.js",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "prettier --check . && publint",
		"format": "prettier --write ."
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^3.0.0 || ^4.0.0"
	},
	"devDependencies": {
		"@sveltejs/package": "^2.3.1",
		"@sveltejs/vite-plugin-svelte": "3.1.0",
		"@testing-library/svelte": "5.0.1",
		"desm": "^1.3.1",
		"happy-dom": "14.7.1",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.3",
		"publint": "^0.2.7",
		"svelte": "^4.2.15",
		"svelte-check": "^3.6.9",
		"svelte-preprocess": "^5.1.4",
		"tslib": "^2.6.2",
		"typescript": "^5.4.5",
		"unpic": "^3.18.0",
		"vitest": "^1.5.0"
	},
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"@cloudinary-util/types": "^1.0.2",
		"@cloudinary-util/url-loader": "5.3.1",
		"@cloudinary-util/util": "3.0.0",
		"@unpic/svelte": "0.0.51",
		"cloudinary": "^2.1.0"
	}
}