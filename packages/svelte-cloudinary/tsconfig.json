{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"noEmit": true,
		"allowImportingTsExtensions": true,
		"types": ["vitest/globals"],
		"lib": ["esnext", "DOM", "DOM.Iterable"],
		"moduleResolution": "Bundler",
		"module": "esnext",
		"target": "esnext"
	}
}
