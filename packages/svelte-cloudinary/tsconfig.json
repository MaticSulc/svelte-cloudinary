{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"noEmit": true,
		"allowImportingTsExtensions": true,
		"moduleResolution": "Bundler",
		"module": "esnext",
		"target": "esnext",
		"verbatimModuleSyntax": true
	}
}
